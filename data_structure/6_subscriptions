-- creating table SUBSCRIPTIONS
CREATE OR REPLACE TABLE SUBSCRIPTIONS (
  SUBSCRIPTION_ID     STRING,
  CUSTOMER_ID         STRING,          
  PLAN_ID             STRING,          
  STATUS              STRING,          
  START_DATE          DATE,
  CANCEL_DATE         TIMESTAMP_NTZ,
  TRIAL_START         DATE,
  TRIAL_END           DATE,
  GAP_FROM_PREV_DAYS  NUMBER(10,0)     
);

--LOAD .CSV TO THE TABLE
COPY INTO SUBSCRIPTIONS
FROM @STG_RAW_FINANCE/subscriptions.csv
ON_ERROR = 'ABORT_STATEMENT';

